    "start": "next start",
    "lint": "next lint",
    "cs:generate-types": "csdx tsgen -a 'delivery_token' -o 'src/types/contentStack/generated.d.ts' --no-doc --include-system-fields --include-referenced-entry --include-editable-tags",
